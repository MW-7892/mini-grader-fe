{
  "name": "mini-grader-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "genTypes": "graphql-codegen --config codegen.yml",
